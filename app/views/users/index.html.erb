<%= render 'list', user: @user , book: @book %>

<div class="col-md-8 offset-md-1">
<table>
    <th>image</th>
    <th>name</th>

<% @users.each do |user| %>
    <tr>
        <td><%= user.name %></td>
    </tr>
    <tr>
        <td><%= user.introduction %></td>
        <td>
        <%=  link_to "Show", user_path(user.id) %>
        </td>
    </tr>
<% end %>
</div>